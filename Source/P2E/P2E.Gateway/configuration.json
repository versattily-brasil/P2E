{
  "GlobalConfiguration": {
    "BaseUrl": "localhost"
  },
  "ReRoutes": [
    // sso
    {
      "DownstreamPathTemplate": "/api/{version}/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": "7010"
        }
      ],
      "UpstreamPathTemplate": "/sso/{version}/{everything}",
      "UpstreamHttpMethod": [ "Get", "Post", "Put", "Delete" ]
    },
    // imp
    {
      "DownstreamPathTemplate": "/api/{version}/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "imp.2e.versattily.com",
          "Port": "80"
        }
      ],
      "UpstreamPathTemplate": "/imp/{version}/{everything}",
      "UpstreamHttpMethod": [ "Get", "Post", "Put", "Delete" ]
    }
  ]
}